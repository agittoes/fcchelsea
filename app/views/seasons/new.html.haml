%h1
  New season

=form_for @season do |f|
  %fieldset
    =f.label :title
    =f.text_field :title

    =f.label :begin_date
    =f.text_field :begin_date

    =f.label :end_date
    =f.text_field :end_date

    =f.collection_select :teams, @teams, :id, :title, :multiple => true

  =f.submit class: ['btn', 'btn-primary', 'btn-large']